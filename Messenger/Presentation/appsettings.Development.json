{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "/logs/log-.txt",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "ConnectionStrings": {
    "Database": "Host=localhost;Port=5432;Database=MessengerDb;Username=postgres;Password=qwerty"
  },
  "JwtSettings": {
    "ValidIssuer": "IRateAPI",
    "ValidAudience": "IRateHttps",
    "Expires": 30,
    "SecretKey": "Secret key which we need to change, mb put in environment",
    "RefreshTokenExpiresDays": 3
  }
}
